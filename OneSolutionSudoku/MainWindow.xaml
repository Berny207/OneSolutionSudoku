<Window x:Class="OneSolutionSudoku.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OneSolutionSudoku" 
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d"
        Title="SudokuGenerator" Height="544" Width="800"
        Background="{DynamicResource BackgroundColor}">
    <Viewbox Stretch="Uniform">
    <Grid>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" x:FieldModifier="public"/>
    </Grid>
    </Viewbox>
</Window>
